{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="mt-5">Panel admina</h1>
            <br>
            <form action="{{ path('newProposal') }}" method="POST" class="form-horizontal">
                <h1 class="mt-5">Nowy wniosek</h1><br>
                <button type="submit" class="btn btn-success">Nowy wniosek</button>
            </form>
            <br>
            {% for proposals in proposals %}
                <a>{{ proposals.name }}</a>
            {% endfor %}
        </div>
    </div>
{% endblock %}